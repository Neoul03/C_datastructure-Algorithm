#include <stdio.h>


int main() {
	int n = 0;
	printf("n : ");
	scanf_s("%d", &n);

	int arr1[16] = {0};
	printf("arr1 : ");
	for (int i = 0; i < n; i++) {
		int a = 0;
		scanf_s("%d", &a);
		arr1[i] = a;
	}

	int arr2[16] = {0};
	printf("arr2 : ");
	for (int i = 0; i < n; i++) {
		int a = 0;
		scanf_s("%d", &a);
		arr2[i] = a;
	}


	for (int i = 0; i < n; i++) {
		int sum = arr1[i] | arr2[i];
		for (int k = n - 1; k >= 0; --k) { //n자리 숫자까지 나타냄
			int result = sum >> k & 1;
			if (result == 1) {
				printf("#");
			}
			else {
				printf(" ");
			}
		}
		printf("\n");
	}
	
	return 0;
}






void printMap(int size, int* arr1, int* arr2) {
	int i = 0, j = 0, row = 0, element = 0;

	for (i = 0; i < size; i++) {
		row = *(arr1 + i) | * (arr2 + i);

		for (j = size - 1; j >= 0; j--) {
			element = row >> j & 1;
			if (element == 1) {
				printf("#");
			}
			else {
				printf(" ");
			}
		}
		printf("\n");
	}
}

void getArr(int* arr) {
	int i = 0;
	char str1[1000] = "";
	char* tok;

	scanf("&s", str1);
	rewind(stdin);

	tok = strtok(str1, ",");
	while (tok != NULL) {
		// *(arr + 3) == arr[3]
		*(arr + i++) = atoi(tok);
		// arr[i] = atoi(tok);
		// i++;
		tok = strtok(NULL, ",");
	}
}
